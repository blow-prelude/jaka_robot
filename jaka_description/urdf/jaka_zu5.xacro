    <!-- 定义传动关系：将关节映射到其物理执行器 -->
    <transmission name="transmission_1">
      <!-- 指定为线性传动关系 -->
      <plugin>transmission_interface/SimpleTransmission</plugin>
      <!-- 指定执行器 -->
      <actuator name="joint_1_motor" role="actuator_1"/>
      <joint name="joint_1" role="joint_1">
        <!-- 在这里定义减速比，例如 1:1 -->
